{
  "$schema": "http://json-schema.org/schema#",
  "title": "insert post",
  "type": "array",
  "items": {
    "type": "object",
    "properties": {
      "title": {
        "type": "string",
        "minLength": 1,
        "maxLength": 64
      },
      "author": {
        "type": "string",
        "maxLength": 8
      },
      "digest": {
        "type": "string",
        "maxLength":120
      },
      "content": {
        "type": "string"
      },
      "thumbMediaId": {
        "type": "string"
      },
      "thumbUrl": {
        "type": "string",
        "minLength": 1,
        "format": "uri"
      },
      "contentSourceUrl": {
        "type": "string",
        "format": "uri"
      }
    },
    "required": [
      "title",
      "digest",
      "content",
      "thumbUrl",
    ],
    "additionalProperties": false
  }
}