{
  "$schema": "http://json-schema.org/schema#",
  "title": "account info",
  "type": "object",
  "properties": {
    "code": {
      "type": "integer",
      "minimum": 200,
      "maximum": 200
    },
    "msg": {
      "type": "string",
      "pattern": "ok"
    },
    "data": {
      "type": "object",
      "properties": {
        "appId": {
          "type": "integer"
        },
        "appSecret": {
          "type": "string"
        },
        "headImg": {
          "type": "string"
        },
        "interest": {
          "type": "array"
        },
        "qrcode": {
          "type": "string"
        },
        "name": {
          "type": "string"
        },
        "type": {
          "type": "integer",
          "minimum": 1,
          "maximum": 2
        }
      },
      "required": [
        "appId",
        "appSecret",
        "headImg",
        "interest",
        "qrcode",
        "name",
        "type"
      ]
    }
  },
  "required": [
    "code",
    "msg",
    "data"
  ]
}